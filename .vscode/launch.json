    "version": "0.2.0",
    "configurations": [
       {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/src/hs.ts",
            "args": [      
                "-q", "g.V().inE()",
                "-d", "C:\\Users\\jakka\\demo\\hyper\\out2.svg"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "build:dev:fast",
            "outFiles": [
                "${workspaceRoot}/lib/**/*.js"
            ],
            "sourceMaps": true
        },
         {
            "type": "node",
            "request": "launch",
            "name": "Debug Test",
            "program": "${workspaceRoot}/test/runTests/testTraversals.ts",
            "args": [      
                "-q", "g.V().inE()",
                "-d", "C:\\Users\\jakka\\demo\\hyper\\out2.svg"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "build:test:fast",
            "outFiles": [
                "${workspaceRoot}/output_test/**/*.js"
            ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Test",
            "program": "${workspaceRoot}/node_modules/ava/profile.js",
            "args": [
                "${workspaceRoot}/output_test/test/runTests/testTraversals.js"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "build:test",
            "outFiles": [
                "${workspaceRoot}/build/output_test/**/*.js"
            ],
            "env":{
                "TestDebug":"true"
            },
            "sourceMaps": true
        }

    ]
